{
  "pages": [
    "pages/index/index",
    "pages/auth/phoneLogin/phoneLogin",
    "pages/auth/login/login",
    "pages/profile/profile",
    "pages/cloud/cloud"
  ],
  "subpackages": [
    {
      "root": "pages2",
      "pages": [
        "pages/recommendSong/recommendSong",
        "pages/songSheetPage/songSheet",
        "pages/songDetail/songDetail"
      ]
    },
    {
      "root": "pages3",
      "pages": [
        "pages/searchPage/search"
      ]
    },
    {
      "root": "pages4",
      "pages": [
        "pages/testWx/test",
        "pages/singleSheetPage/singleSheet"
      ]
    }
  ],
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": [
        "pages2"
      ]
    },
    "pages/cloud/cloud": {
      "network": "all",
      "packages": [
        "pages3"
      ]
    },
    "pages/profile/profile": {
      "network": "all",
      "packages": [
        "pages4"
      ]
    }
  },
  "window": {
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "SixFour",
    "navigationBarTextStyle": "black"
  },
  "tabBar": {
    "custom": true,
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "发现",
        "iconPath": "static/images/tabbar/home.png",
        "selectedIconPath": "static/images/tabbar/home_active.png"
      },
      {
        "pagePath": "pages/cloud/cloud",
        "text": "云村",
        "iconPath": "static/images/tabbar/cloud.png",
        "selectedIconPath": "static/images/tabbar/cloud_active.png"
      },
      {
        "pagePath": "pages/profile/profile",
        "text": "我的",
        "iconPath": "static/images/tabbar/me.png",
        "selectedIconPath": "static/images/tabbar/me_active.png"
      }
    ]
  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "requiredBackgroundModes": [
    "audio"
  ],
  "usingComponents": {
    "music-control": "/components/custom/musicControl/musicControl",
    "music-item": "/components/custom/musicItem/musicItem",
    "top-nav": "/components/common/topNav/topNav",
    "song-sheet": "/components/custom/songSheetComponent/songSheetComponent",
    "song-info-toast": "/components/custom/songInfoToast/songInfoToast"
  },
  "lazyCodeLoading": "requiredComponents"
}